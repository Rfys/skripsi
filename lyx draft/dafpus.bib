	file = {Mohammadi and Taiebat - 2015 - H-Adaptive Updated Lagrangian Approach for Large-D.pdf:D\:\\Zotero\\storage\\DHA2JW9I\\Mohammadi and Taiebat - 2015 - H-Adaptive Updated Lagrangian Approach for Large-D.pdf:application/pdf;Mohammadi and Taiebat - 2015 - H-Adaptive Updated Lagrangian Approach for Large-D.pdf:D\:\\Zotero\\storage\\9NPGYXNS\\Mohammadi and Taiebat - 2015 - H-Adaptive Updated Lagrangian Approach for Large-D.pdf:application/pdf},
